package com.theginbin.ui{	import flash.display.MovieClip;	import com.greensock.TweenMax;	import com.greensock.easing.*;	import flash.events.MouseEvent;	public class RollOverButton extends MovieClip	{		public var over:MovieClip;		public var off:MovieClip;				public function RollOverButton()		{			super();			over.alpha = 0;		}				public function addEvents():void {			this.buttonMode = true;			this.addEventListener(MouseEvent.ROLL_OVER,onButtonRoll, false, 0, true);			this.addEventListener(MouseEvent.ROLL_OUT,onButtonRollOut, false, 0, true);		}				public function removeEvents():void {			this.buttonMode = false;			this.removeEventListener(MouseEvent.ROLL_OVER,onButtonRoll);			this.removeEventListener(MouseEvent.ROLL_OUT,onButtonRollOut);			over.alpha = 0;			off.alpha = 1;		}      	      	public function onButtonRoll(e:MouseEvent):void {        		TweenMax.to(over, 0.4, {alpha:1, ease:Cubic.easeOut});      		//TweenMax.to(off, 0.4, {alpha:0, ease:Cubic.easeOut});      	}      	      	public function onButtonRollOut(e:MouseEvent):void {        		TweenMax.to(over, 0.4, {alpha:0, ease:Cubic.easeOut});      		//TweenMax.to(off, 0.4, {alpha:1, ease:Cubic.easeOut});      	}			}}