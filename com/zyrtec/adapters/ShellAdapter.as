package com.zyrtec.adapters{	import com.adobe.utils.DateUtil;	import com.bigspaceship.utils.Out;	import com.zyrtec.interfaces.IMiniGame;		import flash.events.EventDispatcher;	public class ShellAdapter extends EventDispatcher	{		public var trackingcallback		:Function; 		public var completecallback		:Function; 		public var leaderboardcallback	:Function; 		public var logincallback		:Function;		public var signincallback		:Function; 				public var app:IMiniGame;		// =================================================		// ================ @Callable		// =================================================		public function initialize(									$completecallback:Function, 									$leaderboardcallback:Function, 									$logincallback:Function,									$signincallback:Function 								):void{					completecallback	= 	$completecallback; 						//oc: uncomment for production			leaderboardcallback	=	$leaderboardcallback; 			//			oc: uncomment for development//		 	leaderboardcallback	=	tempLeaderboardCallback; 						logincallback		=	$logincallback; 			signincallback 		= 	$signincallback; 				}		/*Object: {		*					SessionName: String,		*					TimeLeft: 	Int,		*					Rank: 		String,		*					Scores: 	Array: [		* 							Object: {		* 								order:int,		* 								name:String,		* 								score:String		* 							}		* 						]		*			}		*		*/		private function tempLeaderboardCallback($o:Object):Object		{			var scoresArr:Array = [];			var o:Object;			for (var i:int=0; i<5; i++){				o = new Object();				o.order = i+1;				o.name = "Gina B."+(i+1).toString();				o.score = (15693-(i*100)).toString();				scoresArr[i] = o;				trace("&&&&&&&&&&&&&&&&&&&& score"+(15693-(i*100)).toString());				trace("&&&&&&&&&&&&&&&&&&&& order"+(i+1).toString());			}						var data:Object = new Object();			data.SessionName= "Morning Session";			data.TimeLeft	= 2358000;			data.Rank		= "";			data.TopScore 	= "23556";			data.Scores		= scoresArr;			Out.info(new Object(), "TimeLeft: "+data.TimeLeft);			if (app)	app.setLeaderBoard(data);			return	data;				}//end function				public function track($uid:String, $sid:String, $trackId:String, $trackVal:String){						var o:Object 	= new Object();			o.uid			= $uid;			o.sid			= $sid;			o.trackid		= $trackId;			o.trackval		= $trackVal;			trackingcallback(o);					}		// =================================================		// ================ @Workers		// =================================================				// =================================================		// ================ @Handlers		// =================================================			}//end class}//end package