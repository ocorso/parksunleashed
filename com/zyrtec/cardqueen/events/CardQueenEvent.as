package com.zyrtec.cardqueen.events{	import flash.events.Event;	public class CardQueenEvent extends Event {				public static const CARD_CLICKED:String = "cardClicked"; //when a card is clicked		public static const CARD_DOWN:String = "cardDown"; //card down video is done playing		public static const SHUFFLE_COMPLETE:String = "shufflecomplete"; //shuffle sequence is complete		public static const VIDEO_LOADED:String = "videoLoaded"; //dispatched when video is loaded		public static const GAMEOVER_ANIMATION_COMPLETE:String = "gameovercomplete"; //shuffle sequence is complete				public var data:*				///FUNCTION : CONSTRUCTOR		public function CardQueenEvent(type:String, data:*=null){			super(type);			this.data = data;		}				override public function clone():Event {			return new CardQueenEvent(this.type);		} 		//usage:		//dispatchEvent(new BaseballMemoryEvent(BaseballMemoryEvent.TILE_CLICK, _id));	}}