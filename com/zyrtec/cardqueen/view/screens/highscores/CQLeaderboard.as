package com.zyrtec.cardqueen.view.screens.highscores{	import com.greensock.*;	import com.greensock.TweenMax;	import com.greensock.easing.*;	import com.greensock.plugins.*;	import com.theginbin.ui.RollOverButton;	import com.theginbin.utils.BasicFrameTimer;	import com.theginbin.utils.TimeConverter;	import com.zyrtec.events.ZyrtecGameEvent;	import com.zyrtec.model.MiniGameModel;	import com.zyrtec.cardqueen.model.CQModel;	import com.zyrtec.view.Leaderboard;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.IEventDispatcher;	import flash.events.MouseEvent;	import flash.events.TimerEvent;	import flash.utils.Timer;	import flash.net.URLRequest;	import flash.net.navigateToURL;	import flash.external.ExternalInterface;		import com.zyrtec.analytics.Tracking;		// =================================================	// ================ @Class	// =================================================	public class CQLeaderboard extends Leaderboard	{		// =================================================		// ================ @Constructor		// =================================================		public function CQLeaderboard(leaderboardClipMC:MovieClip, gameModel:MiniGameModel)		{			super(leaderboardClipMC, gameModel);		}				// =================================================		// ================ @Callable		// =================================================				//FUNCTION : SET THE LEADERBOARD STATIC TEXT		override public function setStaticText():void {			if (model.isLoggedIn) {  //gray these out				leaderboardClip.signupAreaMC.alpha = 0.4;			}						leaderboardClip.txtLoading.alpha = 1;						leaderboardClip.txtTimeComplete.txt.autoSize = "right";			leaderboardClip.txtTimeComplete.txt.text = model.displayScore;						leaderboardClip.txtRounds.txt.text = CQModel(model).gameLevel;						//display stars earned 			leaderboardClip.txtMinutes.txt.autoSize = "left";			leaderboardClip.txtStars.txt.autoSize = "left";						if (model.score >= 10000) { //3 stars				leaderboardClip.star1.gotoAndStop(2);				leaderboardClip.star2.gotoAndStop(2);				leaderboardClip.star3.gotoAndStop(2);				if (model.score == 10000) {					leaderboardClip.txtMinutes.txt.text = "10,000 points.";				} else {					leaderboardClip.txtMinutes.txt.text = "> 10,000 points.";				}				leaderboardClip.txtStars.txt.text = "3 stars";				model.stars = 3;			} else if (model.score >= 5000 && model.score < 10000){ //2 stars				leaderboardClip.star1.gotoAndStop(2);				leaderboardClip.star2.gotoAndStop(2);				leaderboardClip.star3.gotoAndStop(1);				if (model.score == 5000) {					leaderboardClip.txtMinutes.txt.text = "5,000 points.";				} else {					leaderboardClip.txtMinutes.txt.text = "< 10,000 points.";				}				leaderboardClip.txtStars.txt.text = "2 stars";				model.stars = 2;			} else if (model.score >=1000 && model.score <5000){ //1 star				leaderboardClip.star1.gotoAndStop(2);				leaderboardClip.star2.gotoAndStop(1);				leaderboardClip.star3.gotoAndStop(1);				if (model.score == 1000) {					leaderboardClip.txtMinutes.txt.text = "1,000 points.";				} else {					leaderboardClip.txtMinutes.txt.text = "< 5,000 points.";				}				leaderboardClip.txtStars.txt.text = "1 star";				model.stars = 1;			} else if (model.score<1000) {				leaderboardClip.star1.gotoAndStop(1);				leaderboardClip.star2.gotoAndStop(1);				leaderboardClip.star3.gotoAndStop(1);				leaderboardClip.txtMinutes.txt.text = "< 1,000 points.";				leaderboardClip.txtStars.txt.text = "0 stars";				model.stars = 1;			}		}				//FUNCTION : ON FACEBOOK SHARE CLICK		override public function onFacebookShareClick(e:MouseEvent):void{ //share page on facebook			var title:String ="I unlocked the High Roller Badge. Deal with it.";			var copy:String = "I earned "+model.stars+" stars for scoring an impressive "+model.displayScore+" points playing Follow the Lady at ZYRTEC%26reg; Parks Unleashed%26trade;.";			var url:String = "http://www.facebook.com/sharer.php?s=100&p[title]="+title+"&p[url]="+model.shareURL+"&p[images][0]="+model.badgeURL+"&p[summary]="+copy;			if (ExternalInterface.available){				ExternalInterface.call("openPopup", url, "ShareFacebook", Leaderboard.POP_UP_WIDTH, Leaderboard.POP_UP_HEIGHT);			}else navigateToURL(new URLRequest(url), "_blank");						Tracking.track(Tracking.L3_SHARE_HIGHROLLER);		}	}}