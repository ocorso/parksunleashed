package com.zyrtec.findthebones {		import com.bigspaceship.events.AnimationEvent;	import com.bigspaceship.utils.Out;	import com.greensock.TweenMax;	import com.greensock.easing.*;	import com.zyrtec.adapters.*;	import com.zyrtec.findthebones.controller.FindTheBonesMain;	import com.zyrtec.findthebones.model.FTBGameData;	import com.zyrtec.findthebones.model.FTBModel;	import com.zyrtec.interfaces.*;	import com.zyrtec.findthebones.view.screens.ScreenHighScores;		import flash.display.Bitmap;	import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.media.Sound;	import flash.media.SoundChannel;	import flash.media.SoundTransform;		import net.ored.util.ORedUtils;		// =================================================	// ================ @Class	// =================================================	public class FindTheBonesApp extends MovieClip implements IMiniGame	{		private var _model:FTBModel;		private var _gameMain:FindTheBonesMain;		private var _mainScreen:MovieClip;				//sound vars		private var _muteSoundTransform:SoundTransform = new SoundTransform();		private var _soundMute:Boolean = false;				// =================================================		// ================ @Constructor		// =================================================		public function FindTheBonesApp()		{		//	ORedUtils.turnOutOn();//			Out.info(this, "Welcome to Find the Bones!");//			addEventListener(Event.ADDED_TO_STAGE, onAddedToStage);  // comment this out when using a shell												/* // FTBELL CALLS // */			/*muteGame(false); 						var gd:Object = { //TEST GAME DATA OBJECT				uid:"-1",				sid:"-1",				gid:"-1",				cdn:"",				name:"Becky R.",				isArcadeMode:false,				shareURL:"http://www.facebook.com/"			};					initMiniGame(completecallback, leaderboardcallback, logincallback, signincallback, gd);*/		}		// =================================================		// ================ @Callable		// =================================================					public function onAddedToStage(e:Event){						removeEventListener(Event.ADDED_TO_STAGE, onAddedToStage);  // comment this out when hand off			// FTBELL CALLS // 			muteGame(false); 						var gd:Object = { //TEST GAME DATA OBJECT				uid:"-1",				sid:"-1",				gid:"-1",				cdn:"",				name:"Becky R.",				isArcadeMode:false,				shareURL:"http://www.facebook.com/"			};						initMiniGame(completecallback, leaderboardcallback, logincallback, signincallback, gd);		}				public function initMiniGame($completecallback:Function, $leaderboardcallback:Function, $logincallback:Function, $signincallback:Function, $gamedata:Object):void		{			trace("initMiniGame called");			_model = FTBModel.getInstance();			_model.initialize(this.stage);			_model.setGameData(new FTBGameData($gamedata));						//all shell calls go through the shell adapter via the model.			_model.shell.initialize($completecallback, $leaderboardcallback, $logincallback, $signincallback); 						// ADD main game controller			_gameMain = new FindTheBonesMain(); 			addChild(_gameMain);			_gameMain.init();			FindTheBonesMain(_gameMain).frameRate = 31; //independent from movie frame rate			FindTheBonesMain(_gameMain).startTimer(); ///// START GAMING RUNNING HERE !!!! ////					}		public function setLeaderBoard($o:Object):void		{			_model.leaderboardResults = $o;		}				public function getScore():Object		{			return _model.scoreToSend;		}				public function muteGame($b:Boolean):void		{			if (!$b) {				_soundMute=false;				_muteSoundTransform.volume=1;				flash.media.SoundMixer.soundTransform=_muteSoundTransform;			} else {				_muteSoundTransform.volume=0;				_soundMute = true;				flash.media.SoundMixer.soundTransform=_muteSoundTransform;			}		}				public function gameQuit():Boolean		{			_gameMain.destroy();			return true;		}				public function gamePause($b:Boolean):void		{					}				public function setGameData($o:Object):void{			_model.setGameData(new FTBGameData($o));			ScreenHighScores(_gameMain.gameOverScreen).disableLogInSignUp();			_gameMain.updateLeaderboard();		}				// =================================================		// ================ @Workers		// =================================================				private function signincallback($o:Object=null):void {};				//private function trackingcallback($o:Object=null):void {};				private function leaderboardcallback($o:Object=null):void {					};				private function completecallback():void {}; //game session has ended				private function logincallback():void {}; //show log in screen				// =================================================		// ================ @Handlers		// =================================================	}}