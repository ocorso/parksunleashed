package com.zyrtec.kiteflying.utils{	public class FacebookHelper	{		public static function shareURL($score:String, $numStars:Number, $shareUrl:String, $badgeUrl:String):String{			var p:String = $numStars > 1 ? "s ": " ";			var s:String = "http://facebook.com/sharer.php?";			s			+= "s=100&";			s			+= "p[title]=High Five! I just unlocked the High Flyer Badge!&";			s			+= "p[url]="+ $shareUrl+"&";			s			+= "p[images][0]="+$badgeUrl+"&";			s			+= "p[summary]=I earned "+ $numStars.toString() + " star"+ p + "for getting "+$score+" points and got the High Flyer Badge at ZYRTEC%26reg; Parks Unleashed%26trade;. Ch-ch-check it out.";			s 			= s.replace(" ", "%20");			return s;		}		public static function pianoShareURL($score:String, $numStars:Number, $shareUrl:String, $badgeUrl:String):String{			var p:String = $numStars > 1 ? "s ": " ";			var s:String = "http://facebook.com/sharer.php?";			s			+= "s=100&";			s			+= "p[title]=I'm a rockstar! I unlocked the Ivory Tickler Badge!&";			s			+= "p[url]="+ $shareUrl+"&";			s			+= "p[images][0]="+$badgeUrl+"&";			s			+= "p[summary]=I earned "+ $numStars.toString() + " star"+ p + "for getting "+$score+" correct notes in the Piano Tuning Game at ZYRTEC%26reg; Parks Unleashed%26trade;. You can't outplay this player!";			s 			= s.replace(" ", "%20");			return s;		}	}}