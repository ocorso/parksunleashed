package com.zyrtec.tugofwar.events{	import flash.events.Event;	public class TugOfWarEvent extends Event {						public static const VIDEO_LOADED:String = "videoLoaded"; //dispatched when video is loaded		public static const PATTERN_OBJECT_CLICKED:String = "patternObjectClicked"; //dispatches id of button		public static const BLINK_COMPLETE:String = "blinkComplete"; //when the blink is done in a pattern sequence				public static const DANGER_ZONE:String = "dangerZone"; //when the dog is getting close				public var data:*				///FUNCTION : CONSTRUCTOR		public function TugOfWarEvent(type:String, data:*=null){			super(type);			this.data = data;		}				override public function clone():Event {			return new TugOfWarEvent(this.type);		} 		//usage:		//dispatchEvent(new BaseballMemoryEvent(BaseballMemoryEvent.TILE_CLICK, _id));	}}