package net.ored.util{	public class StringUtils	{		/**		 * This function splits a string up into pieces and returns a vector containing each char 		 * 		 * @param $s 	- the input string is what will be exploded		 * 		 * @return 		- a vector that contains the members of the input string		 * 		 */				public static function explode($s:String):Vector.<String>{			var v:Vector.<String> = new Vector.<String>();			for(var i:uint=0; i<$s.length; i++){	v.push($s.charAt(i));	}			return v;					}//end static function	}}